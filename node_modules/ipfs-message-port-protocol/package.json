{
  "name": "ipfs-message-port-protocol",
  "version": "0.10.5",
  "description": "IPFS client/server protocol over message port",
  "keywords": [
    "ipfs"
  ],
  "homepage": "https://github.com/ipfs/js-ipfs/tree/master/packages/ipfs-message-port-protocol#readme",
  "bugs": "https://github.com/ipfs/js-ipfs/issues",
  "license": "(Apache-2.0 OR MIT)",
  "leadMaintainer": "Alex Potsides <alex@achingbrain.net>",
  "main": "./cjs/src/index.js",
  "types": "types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "types/*",
        "types/src/*"
      ],
      "types/*": [
        "types/*",
        "types/src/*"
      ]
    }
  },
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "publishConfig": {
    "directory": "dist"
  },
  "exports": {
    ".": {
      "browser": "./esm/src/index.js",
      "require": "./cjs/src/index.js",
      "import": "./esm/src/index.js"
    },
    "./block": {
      "browser": "./esm/src/block.js",
      "require": "./cjs/src/block.js",
      "import": "./esm/src/block.js"
    },
    "./cid": {
      "browser": "./esm/src/cid.js",
      "require": "./cjs/src/cid.js",
      "import": "./esm/src/cid.js"
    },
    "./core": {
      "browser": "./esm/src/core.js",
      "require": "./cjs/src/core.js",
      "import": "./esm/src/core.js"
    },
    "./dag": {
      "browser": "./esm/src/dag.js",
      "require": "./cjs/src/dag.js",
      "import": "./esm/src/dag.js"
    },
    "./error": {
      "browser": "./esm/src/error.js",
      "require": "./cjs/src/error.js",
      "import": "./esm/src/error.js"
    }
  },
  "browser": {
    ".": "./cjs/src/index.js",
    "./block": "./cjs/src/block.js",
    "./cid": "./cjs/src/cid.js",
    "./core": "./cjs/src/core.js",
    "./dag": "./cjs/src/dag.js",
    "./error": "./cjs/src/error.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/js-ipfs.git"
  },
  "scripts": {
    "build": "aegir build",
    "test": "aegir test",
    "test:node": "aegir test -t node",
    "test:browser": "aegir test -t browser",
    "test:webworker": "aegir test -t webworker",
    "lint": "aegir ts -p check && aegir lint",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check -i rimraf -i ipfs-core-types"
  },
  "dependencies": {
    "ipfs-core-types": "^0.9.0",
    "multiformats": "^9.4.13"
  },
  "devDependencies": {
    "aegir": "^36.0.1",
    "rimraf": "^3.0.2",
    "uint8arrays": "^3.0.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=3.0.0"
  },
  "gitHead": "7375caa8805298fc671ed4080e1023aafe756151"
}